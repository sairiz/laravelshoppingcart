{
    "name": "saleem189/cart",
    "description": "Laravel 12 Shopping cart with PHP 8.4 support",
    "keywords": ["laravel", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "illuminate/translation": "^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^4.0",
        "laravel/boost": "^1.0",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Darryldecode\\": "src/Darryldecode"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darryldecode\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Darryldecode\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Darryldecode\\Cart\\Facades\\CartFacade"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
